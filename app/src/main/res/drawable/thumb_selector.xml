<?xml version="1.0" encoding="utf-8"?>
<!--Changes thumb drawable depending on whether the switch is checked or not-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true" android:drawable="@drawable/thumb_student" />
    <item android:state_checked="false" android:drawable="@drawable/thumb_faculty" />
</selector>
