{
	"name": "weatherdataload",
	"properties": {
		"activities": [
			{
				"name": "Copy weather json to SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"additionalColumns": [
							{
								"name": "SourceFile",
								"value": "$$FILEPATH"
							}
						],
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"wildcardFileName": "mockFiveDayForecast.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"Date"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Date']"
								},
								"sink": {
									"name": "Date"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Temperature']['Minimum']['Value']"
								},
								"sink": {
									"name": "Temperature_Min_Value"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Temperature']['Minimum']['Unit']"
								},
								"sink": {
									"name": "Temperature_Min_Unit"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Temperature']['Maximum']['Value']"
								},
								"sink": {
									"name": "Temperature_Max_Value"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Temperature']['Maximum']['Unit']"
								},
								"sink": {
									"name": "Temperature_Max_Unit"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['AirAndPollen'][0]['Name']"
								},
								"sink": {
									"name": "AirandPollen_Name"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['AirAndPollen'][0]['Value']"
								},
								"sink": {
									"name": "AirandPollen_Value"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['AirAndPollen'][0]['Category']"
								},
								"sink": {
									"name": "AirandPollen_Category"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Day']['IconPhrase']"
								},
								"sink": {
									"name": "Day_IconPhrase"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Day']['HasPrecipitation']"
								},
								"sink": {
									"name": "Day_HasPrecipitation"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Day']['PrecipitationProbability']"
								},
								"sink": {
									"name": "Day_PrecipitaionProbability"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Day']['Wind']['Speed']['Value']"
								},
								"sink": {
									"name": "Day_WindSpeed_Value"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Day']['Wind']['Speed']['Unit']"
								},
								"sink": {
									"name": "Day_WindSpeed_Unit"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Night']['IconPhrase']"
								},
								"sink": {
									"name": "Night_IconPhase"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Night']['HasPrecipitation']"
								},
								"sink": {
									"name": "Night_HasPrecipitation"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Night']['PrecipitationProbability']"
								},
								"sink": {
									"name": "Night_PrecipitaionProbability"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Night']['Wind']['Speed']['Value']"
								},
								"sink": {
									"name": "Night_WindSpeed_Value"
								}
							},
							{
								"source": {
									"path": "$['DailyForecasts'][0]['Night']['Wind']['Speed']['Unit']"
								},
								"sink": {
									"name": "Night_WindSpeed_Unit"
								}
							},
							{
								"source": {
									"path": "$['SourceFile']"
								},
								"sink": {
									"name": "SourceFile"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "weatherfivedayjson",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "weather_sql",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}